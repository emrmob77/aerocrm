{
  "ci": {
    "collect": {
      "numberOfRuns": 2,
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/dashboard",
        "http://localhost:3000/settings/monitoring"
      ],
      "startServerCommand": "npm run dev -- --port 3000"
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.8
          }
        ],
        "largest-contentful-paint": [
          "warn",
          {
            "maxNumericValue": 3000
          }
        ],
        "interactive": [
          "warn",
          {
            "maxNumericValue": 4000
          }
        ]
      }
    }
  }
}
